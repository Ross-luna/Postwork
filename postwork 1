#ENTRAR A RUTA 
cd "G:\Mi unidad\BEDU"

#CREAR CARPETA
mkdir ProyectoRossL

#ENTAR A CARPETA QUE SE CREO
cd ProyectoRossL

#MOVER CARPETA DE DATOS(ml-1m) A CARPETA DE ProyectoRossL
mv "G:\Mi unidad\BEDU\ml-1m" "G:\Mi unidad\BEDU\ProyectoRossL"

#CREAR RESPALDO
cp -a "G:\Mi unidad\BEDU\ProyectoRossL" "G:\Mi unidad\BEDU\ProyectoRossL_1"

#MUESTRA ARCHIVOS EN CARPETA
ls
ml-1m/

#SALIR DE CARPETA PoryectoRossL
cd ..

#MOSTRAR CARPETAS

ls

#CREAR TXT
touch holaBedu.txt

#MOVER TXT A CARPETA ProyectoRossL

mv holabedu.txt ProyectoRossL

#MOSTRAR CONTENIDO DE LA CARPETA
ls

#ENTRAR A CARPETA
cd ProyectoRossL

#MOSTRAR CONTENIDO
ls

